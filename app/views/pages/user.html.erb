<div class="container">
  <% @users.each do |user| %>
    <div class="row justify-content-center mb-3">
      <div class="col-6">
        <div class="card-clients">
          <div class="ml-auto p-5 w-100">
            <div class="client-info d-flex justify-content-between align-items-center">
              <h2><%= "#{user.first_name} #{user.last_name}" %></h2>
            </div>
          </div>
        </div>
      </div>
    </div>
  <% end %>
</div>
